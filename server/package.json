  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "lint": "tsc --noEmit && eslint \"{src,apps,libs,test,utils}/**/*.ts\" --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "seed": "dotenv -e .env.development -- npx prisma db seed",
    "removeSetRegistrationAddresses": "ts-node prisma/remove-setRegistrationAddress"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.2",
    "@aws-sdk/client-s3": "^3.94.0",
    "@logtail/node": "^0.1.12",
    "@logtail/winston": "^0.1.13",
    "@nestjs-modules/mailer": "^1.7.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/throttler": "^2.0.1",
    "@prisma/client": "^4.3.1",
    "@types/validator": "^13.7.2",
    "archiver": "^5.3.1",
    "cache-manager": "^3.6.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-pg-simple": "^7.0.0",
    "cross-env": "^7.0.3",
    "csv-writer": "^1.6.0",
    "dotenv-cli": "^5.1.0",
    "dotenv-flow": "^3.2.0",
    "ethers": "^5.6.6",
    "express-session": "^1.17.3",
    "helmet": "^5.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nest-winston": "^1.7.0",
    "nodemailer": "^6.7.5",
    "passport": "^0.5.3",
    "passport-custom": "^1.1.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "prisma": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.4.0",
    "twitter-lite": "^1.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@types/archiver": "^5.3.1",
    "@types/cache-manager": "^3.4.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.34",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": "16.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
